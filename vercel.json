{
  "buildCommand": "cd frontend && DISABLE_ESLINT_PLUGIN=true CI=false npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install && cd frontend && npm install",
  "rewrites": [
    {
      "source": "/api/simple-profile",
      "destination": "/api/simple-profile.js"
    },
    {
      "source": "/api/test-profile",
      "destination": "/api/test-profile.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 10
    }
  }
}