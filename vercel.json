{
  "buildCommand": "cd frontend && DISABLE_ESLINT_PLUGIN=true CI=false npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install && cd frontend && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/supabase-handler.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/supabase-handler.js": {
      "runtime": "edge"
    }
  }
}